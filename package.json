{
  "name": "watchman",
  "description": "A core set of utilities frequently used in large data processing / ML projects, exposed as REST endpoints",
  "version": "1.0.0",
  "main": "server/server.js",
  "scripts": {
    "postinstall": "npm run build",
    "pretest": "jshint .",
    "dev": "nodemon . -w server -w common -w lib --ext js,json",
    "build": "bower install && npm run build:env",
    "build:env": "mkdir -p client/build && ./script/deploy/env.js > client/build/env.js"
  },
  "private": true,
  "engines": {
    "node": "6.9.x",
    "npm": "3.10.x"
  },
  "dependencies": {
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.0",
    "compression": "^1.0.3",
    "cors": "^2.5.2",
    "debug": "^2.2.0",
    "dotenv": "^2.0.0",
    "extract-zip": "^1.5.0",
    "feedparser": "^1.1.4",
    "jade": "^1.11.0",
    "kue": "^0.10.5",
    "lodash": "^4.6.1",
    "loopback": "^2.22.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^2.1.0",
    "loopback-connector-mongodb": "~1.15.0",
    "loopback-datasource-juggler": "^2.39.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.12.0",
    "mstranslator": "^2.1.2",
    "multiparty": "^4.1.2",
    "node-dir": "^0.1.14",
    "node-geocoder": "^3.8.0",
    "node-ner": "git+https://github.com/Sotera/node-ner.git",
    "node-red": "^0.16.2",
    "node-red-dashboard": "^2.3.10",
    "redis": "^2.6.2",
    "request": "^2.69.0",
    "request-json": "^0.6.1",
    "request-json-light": "^0.5.22",
    "serve-favicon": "^2.0.1",
    "textract": "^1.2.1",
    "twitter": "^1.7.0"
  },
  "devDependencies": {
    "nodemon": "^1.9.2"
  },
  "repository": {
    "type": "",
    "url": "https://github.com/Sotera/watchman"
  }
}
