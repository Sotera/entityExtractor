   "source": [
    "# calc pmi for each pair using top x hashtags-posts\n",
    "\n",
    "posts_count = df.count()\n",
    "posts_count\n",
    "\n",
    "pmi_parts = []\n",
    "    \n",
    "for p in top_pairs:\n",
    "    a=ds_posts_by_tag.loc[ds_posts_by_tag['_tag'] == p[0]]\n",
    "    b=ds_posts_by_tag.loc[ds_posts_by_tag['_tag'] == p[1]]\n",
    "    post_ids_0 = list(a.post_ids)[0]\n",
    "    post_ids_1 = list(b.post_ids)[0]\n",
    "    \n",
    "    # [tag pair, count both, count a, count b]\n",
    "    pmi_parts.append([p, len(set(post_ids_0) & set(post_ids_1)), len(post_ids_0), len(post_ids_1), posts_count])\n",
    "\n",
    "# pmi_parts\n",
    "\n",
    "pmi_scores = []\n",
    "\n",
    "for p in pmi_parts:\n",
    "    if not(p[1] and p[2] and p[3]):\n",
    "        continue\n",
    "    score = np.log2(( p[1]*posts_count)/ (p[2]*p[3]))\n",
    "    pmi_scores.append([p[0], score])\n",
    "    \n",
    "sorted_scores = sorted(pmi_scores, key= lambda x: x[1], reverse=1)\n",
    "sorted_scores"
   ]
  },
