extends /modules/core/views/layouts/layout
block content
  .row
    .col-md-12
      div(ng-controller='CoreCtrl')
        form(role='form')
          .form-group
            label(for='dataString') Enter text to parse
            input#dataString.form-control(name='dataString' ng-model='myModel.dataString')


        button.btn.btn-default(ng-click='submit()') Submit

        button.btn.btn-default(ng-click='processNewsFeed(true)') Start News Feed
        //- button.btn.btn-default(ng-click='processNewsFeed(false)') Stop News Feed

        leaflet(style='height:512px' center='center' tiles='tiles' markers='markers')
        button.btn.btn-default(ng-click='destroyData()') Destroy data

