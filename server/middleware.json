{
  "initial:before": {
    "loopback#favicon": {}
  },
  "initial": {
    "compression": {
      "enabled": false
    },
    "cors": {
      "params": {
        "origin": true,
        "credentials": true,
        "maxAge": 86400
      }
    }
  },
  "session": {},
  "auth": {},
  "parse": {
    "body-parser#json": {
      "params": {
        "limit": "500mb"
      }
    },
    "body-parser#urlencoded": {
      "params": {
        "limit": "500mb",
        "extended": true
      }
    }
  },
  "routes": {
    "loopback#rest": {
      "paths": [
        "${restApiRoot}"
      ]
    }
  },
  "files": {
    "loopback#static": {
      "params": "$!../client"
    }
  },
  "final": {
    "loopback#urlNotFound": {}
  },
  "final:after": {
    "loopback#errorHandler": {}
  }
}
