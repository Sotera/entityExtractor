{
  "name": "textFeed",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "indexes": {
    "uniq_idx": {
      "keys": {
        "url": 1,
        "extractType": 1
      },
      "options": {
        "unique": true
      }
    }
  },
  "properties": {
    "url": {
      "type": "string",
      "required": true
    },
    "feedType": {
      "type": "string",
      "default": "rss",
      "required": true
    },
    "extractType": {
      "type": "string",
      "default": "mitie",
      "required": true
    },
    "locale": {
      "type": "string"
    },
    "interval": {
      "type": "number",
      "default": 60000
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
