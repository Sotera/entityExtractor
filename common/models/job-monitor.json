{
  "name": "jobMonitor",
  "base": "PersistedModel",
  "idInjection": true,
  "indexes": {
    "idx_uniq": {
      "keys": {
        "start_time": 1,
        "end_time": 1,
        "featurizer": 1,
        "lang": 1
      },
      "options": {
        "unique": true
      }
    }
  },
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Timestamp": {}
  },
  "properties": {
    "start_time": {
      "type": "number",
      "required": true,
      "description": "start time of associated created (NOT job start time)"
    },
    "end_time": {
      "type": "number",
      "required": true,
      "description": "end time of associated created (NOT job end time)"
    },
    "featurizer": {
      "type": "string",
      "required": true
    },
    "lang": {
      "type": "string"
    },
    "state": {
      "type": "string",
      "required": true,
      "default": "new"
    },
    "done_at": {
      "type": "date"
    },
    "error_msg": {
      "type": "string"
    },
    "start": {
      "type": "boolean",
      "default": true,
      "required": true,
      "description": "trigger the monitoring process"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
