{
  "name": "socialMediaPost",
  "base": "PersistedModel",
  "idInjection": true,
  "indexes": {
    "idx_uniq": {
      "keys": {
        "post_id": 1,
        "post_type": 1
      },
      "options": {
        "unique": true
      }
    },
    "idx_timestamp_ms": {
      "keys": {
        "timestamp_ms": 1
      }
    },
    "idx_state": {
      "keys": {
        "state": 1
      }
    },
    "idx_featurizers": {
      "keys": {
        "featurizers": 1
      }
    }
  },
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Timestamp": {}
  },
  "properties": {
    "post_id": {
      "type": "string",
      "required": true,
      "description": "external id from social media service"
    },
    "post_type": {
      "type": "string",
      "required": true,
      "description": "twitter, instagram, etc."
    },
    "post_url": {
      "type": "string",
      "required": true,
      "description": "original page url"
    },
    "text": {
      "type": "string",
      "default": ""
    },
    "lang": {
      "type": "string",
      "default": ""
    },
    "hashtags": {
      "type": [
        "string"
      ],
      "default": []
    },
    "image_urls": {
      "type": [
        "string"
      ],
      "default": []
    },
    "featurizers": {
      "type": [
        "string"
      ],
      "default": []
    },
    "timestamp_ms": {
      "type": "string",
      "required": true
    },
    "state": {
      "type": "string",
      "required": true
    },
    "image_features": {
      "type": [
        "number"
      ],
      "default": []
    },
    "text_features": {
      "type": [
        "number"
      ],
      "default": []
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
