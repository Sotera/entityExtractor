[
  {
    "name": "data-container",
    "Image": "lukewendling/data-container:2",
    "DockerFilePath": "",
    "Hostname": "data-container"
  },
  {
    "name": "redis",
    "Image": "redis:3.0",
    "DockerFilePath": "",
    "Hostname": "redis",
    "HostConfig": {
      "PortBindings": {
        "6379/tcp": [
          {
            "HostPort": "6379"
          }
        ]
      }
    }
  },
  {
    "name": "caffe",
    "Image": "caffe_redis_client:1",
    "DockerFilePath": "",
    "Hostname": "caffe",
    "HostConfig": {
      "VolumesFrom": [
        "data-container"
      ],
      "Links": [
        "redis:redis"
      ]
    }
  }
]
