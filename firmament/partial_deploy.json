[
  {
    "name": "data-container",
    "Image": "lukewendling/data-container:2",
    "DockerFilePath": "",
    "Hostname": "data-container",
    "HostConfig": {
      "Binds": ["/downloads:/downloads"]
    }
  },
  {
    "name": "redis",
    "Image": "sotera/redis-watchman:1",
    "DockerFilePath": "",
    "Hostname": "redis",
    "HostConfig": {
      "PortBindings": {
        "6379/tcp": [
          {
            "HostPort": "6379"
          }
        ]
      }
    }
  },
  {
    "name": "mongo",
    "Image": "jreeme/mongo:2.6",
    "DockerFilePath": "",
    "Hostname": "mongo",
    "HostConfig": {
      "VolumesFrom": [
        "data-container"
      ],
      "PortBindings": {
        "27017/tcp": [
          {
            "HostPort": "27017"
          }
        ]
      }
    }
  },
  {
    "name": "watchman-featurizer",
    "Image": "lukewendling/strong-pm:1",
    "DockerFilePath": "",
    "Hostname": "watchman-featurizer",
    "ExposedPorts": {
    },
    "HostConfig": {
      "VolumesFrom": [
        "data-container"
      ],
      "Links": [
        "mongo:mongo",
        "redis:redis"
      ],
      "PortBindings": {
        "8701/tcp": [
          {
            "HostPort": "8704"
          }
        ]
      }
    },
    "ExpressApps": [
      {
        "GitUrl": "https://github.com/Sotera/watchman.git",
        "GitSrcBranchName": "master",
        "StrongLoopBranchName": "deploy",
        "StrongLoopServerUrl": "http://localhost:8704",
        "ServiceName": "watchman-featurizer",
        "ClusterSize": 1,
        "EnvironmentVariables": {
          "NODE_ENV": "production",
          "WORKER_SCRIPT": "./workers/featurizer",
          "ES_HOST": "elasticsearch:9200"
        },
        "Scripts":[

        ]
      }
    ]
  }
]
